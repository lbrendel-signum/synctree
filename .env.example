# InvenTree Configuration (Required)
INVENTREE_SERVER_URL=https://demo.inventree.org
INVENTREE_TOKEN=your-inventree-api-token-here

# Digikey Configuration (Optional - at least one supplier required)
DIGIKEY_CLIENT_ID=your-digikey-client-id-here
DIGIKEY_CLIENT_SECRET=your-digikey-client-secret-here
DIGIKEY_STORAGE_PATH=/tmp/digikey  # Optional, defaults to ~/.synctree/.digikey
DIGIKEY_CLIENT_SANDBOX=False  # Optional, defaults to False

# Mouser Configuration (Optional - at least one supplier required)
MOUSER_PART_API_KEY=your-mouser-part-api-key-here
